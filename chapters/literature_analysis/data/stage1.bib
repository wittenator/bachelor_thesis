 language = {en},
 pages = {175-186},
 publisher = {{Springer International Publishing}},
 series = {Studies in {{Applied Philosophy}}, {{Epistemology}} and {{Rational Ethics}}},
 title = {Modelling {{Machine Learning Models}}},
 year = {2018}
